{
  "name": "event-management-system-api",
  "version": "0.1.0",
  "bin": {
    "api": "bin/CdkApp.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --coverage=true --silent",
    "test:watch": "jest --watchAll --coverage=true --silent",
    "cdk": "cdk",
    "lint": "prettier -c 'lib/**/*{.ts,.js}' && prettier -c 'lambdas/**/*{.ts,.js}' && prettier --write 'test/**/*{.ts,.js}'",
    "lint:fix": "prettier --write 'lib/**/*{.ts,.tsx,.js}' && prettier --write 'lambdas/**/*{.ts,.js}' && prettier --write 'test/**/*{.ts,.js}'"
  },
  "devDependencies": {
    "@types/node": "10.17.5",
    "aws-cdk": "1.27.0",
    "aws-sdk": "^2.773.0",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "1.66.0",
    "@aws-cdk/aws-cognito": "1.66.0",
    "@aws-cdk/aws-iam": "1.66.0",
    "@aws-cdk/aws-lambda": "1.66.0",
    "@aws-cdk/aws-lambda-event-sources": "1.66.0",
    "@aws-cdk/core": "1.66.0",
    "source-map-support": "^0.5.16"
  }
}
